{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/dynamic-compose.json",
  "services": [
    {
      "name": "prowlarr",
      "image": "ghcr.io/11notes/prowlarr:2.3.0",
      "isMain": true,
      "internalPort": "9696",
      "readOnly": true,
      "securityOpt": [
        "no-new-privileges=true"
      ],
      "environment": {
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "${TZ}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/prowlarr/etc"
        }
      ]
    }
  ]
}
